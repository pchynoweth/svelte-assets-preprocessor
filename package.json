{
  "name": "svelte-assets-preprocessor",
  "version": "0.0.2",
  "description": "Assets preprocessor for Svelte JS.",
  "engines": {
    "node": ">= 12.13 <13"
  },
  "main": "build/src/main.js",
  "types": "build/src/main.d.ts",
  "files": [
    "build/src"
  ],
  "devDependencies": {
    "@types/jest": "~25.2.1",
    "@types/node": "~12.12.38",
    "@typescript-eslint/eslint-plugin": "~2.32.0",
    "@typescript-eslint/parser": "~2.32.0",
    "eslint": "~7.0.0",
    "eslint-config-prettier": "~6.11.0",
    "eslint-plugin-jest": "~23.10.0",
    "jest": "~25.2.7",
    "prettier": "~2.0.5",
    "rimraf": "~3.0.2",
    "ts-jest": "~25.5.1",
    "tsutils": "~3.17.0",
    "typescript": "~3.8.3"
  },
  "scripts": {
    "clean": "rimraf coverage dist tmp",
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "prepare": "npm run build"
  },
  "author": "Peter Chynoweth <24738364+pchynoweth@users.noreply.github.com>",
  "license": "MIT",
  "dependencies": {
    "node-html-parser": "^1.2.17",
    "svelte": "^3.23.0",
    "tslib": "~1.11.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pchynoweth/svelte-assets-preprocessor.git"
  },
  "bugs": {
    "url": "https://github.com/pchynoweth/svelte-assets-preprocessor/issues"
  },
}
